{
  "name": "mini-docs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run copy-templates && npm run copy-bin && node ./dist/bin/mini-docs-cli.js -i test/test-docs -o test/test-output",
    "build": "tsc",
    "copy-templates": "copyfiles -u 1 ./src/templates/* dist && copyfiles -u 1 ./src/templates/partials/* dist",
    "copy-bin": "copyfiles -f src/bin/mini-docs-cli.js dist/bin"
  },
  "keywords": [
    "mini-docs",
    "documentation",
    "cli",
    "markdown",
    "static site generator"
  ],
  "author": "Ryan Henness",
  "license": "ISC",
  "dependencies": {
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0",
    "marked": "^13.0.2"
  },
  "bin": {
    "mini-docs": "./dist/bin/mini-docs-cli.js"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.14.10",
    "copyfiles": "^2.4.1",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
